{
    "prefix":{
        "inheritable": true,
        "default": null,
        "allowed_on": ["class", "struct", "namespace", "enum"]
    },
    "file":{
        "inheritable": true,
        "default": "{file_name}",
        "allowed_on": ["class", "struct", "namespace", "enum"]
    },
    "file_full_name":{
        "inheritable": true,
        "default": "{file_full_name}",
        "allowed_on": ["class", "struct", "namespace", "enum"]
    },
    "module":
    {
        "inheritable": false,
        "default": "{module_name}",
        "allowed_on": ["class", "struct", "namespace", "enum"]
    },
    "package":
    {
        "inheritable": true,
        "default": "undefined",
        "allowed_on": ["class", "struct", "namespace", "enum"]
    },
    "name":
    {
        "inheritable": false,
        "default": "{object_name}"
    },
    "wrapper":
    {
        "inheritable": false,
        "default": "{module_name}_{object_name}",
        "allowed_on": ["cxx_method", "function"]
    },
    "include":
    {
        "inheritable": true,
        "array": true,
        "default": [],
        "allowed_on": ["namespace"]
    },
    "shared_ref":
    {
        "inheritable": false,
        "default": false,
        "allowed_on": ["class", "struct"]
    }
}
